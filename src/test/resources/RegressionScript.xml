<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="iNUPCORegressionTestSuite" verbose="1" parallel="false">
    <test name="Shipping Tests">
        <groups>
            <run>
                <include name="${groups}"/>
            </run>
        </groups>
        <packages>
            <package name="com.nupco.tests.InboundFlowsTests"/>
            <package name="com.nupco.tests.outboundFlowsTests"/>
        </packages>
    </test>
</suite>
